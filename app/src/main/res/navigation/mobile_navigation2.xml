<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/nav_home">



    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.app.ui.home.HomeFragment"
        android:label="Add section"
        tools:layout="@layout/fragment_create_section"

        />


    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.app.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile"

        />

    <fragment
    android:id="@+id/nav_my_sections"
    android:name="com.example.app.ui.mySections.MySectionsFragment"
    android:label="My sections"
    tools:layout="@layout/fragment_my_sections" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.app.ui.sections.Sections"
        android:label="Sections"
        tools:layout="@layout/fragment_section" />

    <fragment
        android:id="@+id/nav_out"
        android:name="com.example.app.ui.out.OutFragment"
        android:label="Out"
        tools:layout="@layout/fragment_out" />







</navigation>
